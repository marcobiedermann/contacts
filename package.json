{
  "name": "contacts",
  "version": "1.0.0",
  "description": "Contact management tool built using React, Firebase and TypeScript.",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.production.config.ts",
    "check": "tsc --noEmit",
    "develop": "webpack serve --config webpack.development.config.ts",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:css": "tcm -p 'src/**/*.module.css' .",
    "generate": "npm run generate:css",
    "lint:js": "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:js",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "test": "jest --color --runInBand",
    "test:ci": "npm run test -- --ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/contacts.git"
  },
  "keywords": [
    "app",
    "contacts",
    "firebase",
    "react",
    "typescript"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/contacts/issues"
  },
  "homepage": "https://github.com/marcobiedermann/contacts#readme",
